{
    "global": {
        "check_for_updates_on_startup": true,
        "show_in_menu_bar": true,
        "show_profile_name_in_menu_bar": false
    },
    "profiles": [
        {
            "complex_modifications": {
                "parameters": {
                    "basic.simultaneous_threshold_milliseconds": 50,
                    "basic.to_delayed_action_delay_milliseconds": 500,
                    "basic.to_if_alone_timeout_milliseconds": 1000,
                    "basic.to_if_held_down_threshold_milliseconds": 500,
                    "mouse_motion_to_scroll.speed": 100
                },
                "rules": [
                    {
                      "description": "CapsLock to Fn/Escape",
                      "manipulators": [
                        {
                          "type": "basic",
                          "from": {
                            "key_code": "caps_lock",
                            "modifiers": {
                              "optional": ["any"]
                            }
                          },
                          "to": [
                            {
                              "key_code": "fn",
                              "modifiers": ["any"]
                            }
                          ],
                          "to_if_alone": [
                            {
                              "key_code": "escape"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "description": "Change Fn + i/j/k/l to Arrows",
                      "manipulators": [
                        {
                          "type": "basic",
                          "from": {
                            "key_code": "j",
                            "modifiers": {
                              "mandatory": ["fn"],
                              "optional": ["any"]
                            }
                          },
                          "to": [
                            {
                              "key_code": "left_arrow"
                            }
                          ]
                        },
                        {
                          "type": "basic",
                          "from": {
                            "key_code": "k",
                            "modifiers": {
                              "mandatory": ["fn"],
                              "optional": ["any"]
                            }
                          },
                          "to": [
                            {
                              "key_code": "down_arrow"
                            }
                          ]
                        },
                        {
                          "type": "basic",
                          "from": {
                            "key_code": "i",
                            "modifiers": {
                              "mandatory": ["fn"],
                              "optional": ["any"]
                            }
                          },
                          "to": [
                            {
                              "key_code": "up_arrow"
                            }
                          ]
                        },
                        {
                          "type": "basic",
                          "from": {
                            "key_code": "l",
                            "modifiers": {
                              "mandatory": ["fn"],
                              "optional": ["any"]
                            }
                          },
                          "to": [
                            {
                              "key_code": "right_arrow"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "description": "Change Fn + h/x to Backspace and Del",
                      "manipulators": [
                        {
                          "type": "basic",
                          "from": {
                            "key_code": "x",
                            "modifiers": {
                              "mandatory": ["fn"],
                              "optional": ["any"]
                            }
                          },
                          "to": [
                            {
                              "key_code": "delete_forward"
                            }
                          ]
                        },
                        {
                          "type": "basic",
                          "from": {
                            "key_code": "h",
                            "modifiers": {
                              "mandatory": ["fn"],
                              "optional": ["any"]
                            }
                          },
                          "to": [
                            {
                              "key_code": "delete_or_backspace"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "description": "Fn + Command + t to open Telegram",
                      "manipulators": [
                        {
                          "type": "basic",
                          "from": {
                            "key_code": "t",
                            "modifiers": {
                              "mandatory": ["fn", "command"]
                            }
                          },
                          "to": [
                            {
                              "shell_command": "open '/Applications/Telegram.app'"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "description": "Fn + Command + f to open Firefox",
                      "manipulators": [
                        {
                          "type": "basic",
                          "from": {
                            "key_code": "f",
                            "modifiers": {
                              "mandatory": ["fn", "command"]
                            }
                          },
                          "to": [
                            {
                              "shell_command": "open '/Applications/Firefox.app'"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "description": "Fn + Command + S to open Spotify",
                      "manipulators": [
                        {
                          "type": "basic",
                          "from": {
                            "key_code": "s",
                            "modifiers": {
                              "mandatory": ["fn", "command"]
                            }
                          },
                          "to": [
                            {
                              "shell_command": "open '/Applications/Spotify.app'"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "description": "Fn + Command + C to open Chromium",
                      "manipulators": [
                        {
                          "type": "basic",
                          "from": {
                            "key_code": "c",
                            "modifiers": {
                              "mandatory": ["fn", "command"]
                            }
                          },
                          "to": [
                            {
                              "shell_command": "open '/Applications/Chromium.app'"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "description": "Fn + O to open Kitty",
                      "manipulators": [
                        {
                          "type": "basic",
                          "from": {
                            "key_code": "o",
                            "modifiers": {
                              "mandatory": ["fn"]
                            }
                          },
                          "to": [
                            {
                              "shell_command": "open '/Applications/kitty.app'"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "description": "Change input source to English by left_command, Russian by right_command",
                      "manipulators": [
                        {
                          "from": {
                            "key_code": "left_command",
                            "modifiers": {
                              "optional": ["any"]
                            }
                          },
                          "to": [
                            {
                              "key_code": "left_command"
                            }
                          ],
                          "to_if_alone": [
                            {
                              "select_input_source": {
                                "language": "^en$"
                              }
                            }
                          ],
                          "type": "basic"
                        },
                        {
                          "from": {
                            "key_code": "right_command",
                            "modifiers": {
                              "optional": ["any"]
                            }
                          },
                          "to": [
                            {
                              "key_code": "right_command"
                            }
                          ],
                          "to_if_alone": [
                            {
                              "select_input_source": {
                                "language": "^ru$"
                              }
                            }
                          ],
                          "type": "basic"
                        }
                      ]
                    },
                    {
                      "description": "Fn + square brackets to volume control",
                      "manipulators": [
                        {
                          "from": {
                            "key_code": "open_bracket",
                            "modifiers": {
                              "mandatory": "fn"
                            }
                          },
                          "to": [
                            {
                              "consumer_key_code": "volume_decrement"
                            }
                          ],
                          "type": "basic"
                        },
                        {
                          "from": {
                            "key_code": "close_bracket",
                            "modifiers": {
                              "mandatory": "fn"
                            }
                          },
                          "to": [
                            {
                              "consumer_key_code": "volume_increment"
                            }
                          ],
                          "type": "basic"
                        }
                      ]
                    },
                    {
                      "description": "Fn + curly brackets to brightness control",
                      "manipulators": [
                        {
                          "from": {
                            "key_code": "open_bracket",
                            "modifiers": {
                              "mandatory": [ "fn", "shift" ]
                            }
                          },
                          "to": [
                            {
                              "consumer_key_code": "display_brightness_decrement"
                            }
                          ],
                          "type": "basic"
                        },
                        {
                          "from": {
                            "key_code": "close_bracket",
                            "modifiers": {
                              "mandatory": [ "fn", "shift" ]
                            }
                          },
                          "to": [
                            {
                              "consumer_key_code": "display_brightness_increment"
                            }
                          ],
                          "type": "basic"
                        }
                      ]
                    },
                    {
                      "description": "Fn + Backslash to play/pause",
                      "manipulators": [
                        {
                          "from": {
                            "key_code": "backslash",
                            "modifiers": {
                              "mandatory": "fn"
                            }
                          },
                          "to": [
                            {
                              "consumer_key_code": "play_or_pause"
                            }
                          ],
                          "type": "basic"
                        }
                      ]
                    },
                    {
                      "description": "Fn + Command + squared brackets to fastforward and rewind",
                      "manipulators": [
                        {
                          "from": {
                            "key_code": "open_bracket",
                            "modifiers": {
                              "mandatory": [ "fn", "command" ]
                            }
                          },
                          "to": [
                            {
                              "consumer_key_code": "rewind"
                            }
                          ],
                          "type": "basic"
                        },
                        {
                          "from": {
                            "key_code": "close_bracket",
                            "modifiers": {
                              "mandatory": [ "fn", "command" ]
                            }
                          },
                          "to": [
                            {
                              "consumer_key_code": "fastforward"
                            }
                          ],
                          "type": "basic"
                        }
                      ]
                    }

                  ]
            },
            "devices": [
                {
                    "disable_built_in_keyboard_if_exists": false,
                    "fn_function_keys": [],
                    "identifiers": {
                        "is_keyboard": true,
                        "is_pointing_device": false,
                        "product_id": 630,
