.+,v

CVS
\.\#.+       # CVS conflict files / emacs lock files
\.cvsignore

\.svn
\.vscode
_darcs
\.hg

\.git
\.gitignore
\.gitmodules

.+.local


.+~          # emacs backup files
\#.*\#       # emacs autosave files

^/README.*
^/LICENSE.*
^/installer.*
^/scripts.*
^/assets.*
^