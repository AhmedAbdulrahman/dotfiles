{
	"name": "vsnip",
	"author": "Ahmed Abdulrahman",
	"engines": {
		"vscode": "^1.11.0"
	},
	"contributes": {
		"snippets": [
			{
				"language": "css",
				"path": "./css.json"
			},
			{
				"language": "global",
				"path": "./global.json"
			},
			{
				"language": "html",
				"path": "./html.json"
			},
			{
				"language": "python",
				"path": "./python.json"
			},
			{
				"language": [
					"javascript",
					"javascriptreact",
					"vue"
				],
