			},
			{
				"language": [
					"typescript",
					"typescriptreact"
				],
				"path": "./tsc.json"
			},
			{
				"language": "vim",
				"path": "./vim.json"
			},
			{
				"language": "lua",
				"path": "./lua.json"
			},
			{
				"language": "markdown",
				"path": "./markdown.json"
			}
